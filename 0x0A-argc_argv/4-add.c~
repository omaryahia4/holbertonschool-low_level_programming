#include <stdio.h>
#include <stdlib.h>
/**
* main - adds positive numbers..
* @argc: number of arguments.
* @argv: arguments.
* Return: Always 0.
*/

int main(int argc, char *argv[])
{
	int i, j, sum;

	if (argc > 1)
	{
		for (i = 1; i < argc; i++)
		{
			for (j = 0; argv[i][j] != '\0'; j++)
			{
				sum = 0;
				if (!(argv[i][j] >= '0' && argv[i][j] <= '9'))
					{
						printf("Error\n");
						return (1);
					}
			}
					Sum += atoi(argv[i]);
		}
	}
	else 
	{
		printf("0\n");
	}
	
		return (0);
}
